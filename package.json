{
  "name": "healthy-habits",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "node server/setup.js",
    "dev": "concurrently -n \"api,ui\" -c \"cyan,green\" \"node --watch server/index.js\" \"vite\"",
    "build": "tsc -b && vite build",
    "start": "concurrently -n \"api,ui\" -c \"cyan,green\" \"node server/index.js\" \"vite preview\""
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.2.1",
    "typescript": "~5.7.0",
    "vite": "^6.0.0"
  }
}
